</span><span class='line'>&lt;pre>&lt;code> /* function method controller */
</span><span class='line'> $staticPages = array(
</span><span class='line'>                 'test'
</span><span class='line'> );
</span><span class='line'>
</span><span class='line'> //&lt;-- *  Pages Default * --&gt;
</span><span class='line'> if( !in_array( $page, $staticPages ) 
</span><span class='line'>     &amp;&amp; isset( $page ) 
</span><span class='line'> ) {
</span><span class='line'>      $this-&gt;_c $page );
</span><span class='line'>     $this-&gt;_method     = strtolower( $page );
</span><span class='line'>
</span><span class='line'> }
</span><span class='line'> //&lt;--- ************* Pages ********* ---&gt;
</span><span class='line'> else if( isset( $page ) 
</span><span class='line'>         &amp;&amp; in_array( $page, $staticPages )
</span><span class='line'> ) {
</span><span class='line'>     //load nama controller adalah test = load nama folder test di dalam folder views, jadi harus sama
</span><span class='line'>      $this-&gt;_c
</span><span class='line'>     $this-&gt;_method     = strtolower( $page );
</span><span class='line'> }
</span><span class='line'>
</span><span class='line'> //&lt;-- * if no have variables defined $ _GET showed index * --&gt;
</span><span class='line'> if( !$this-&gt;_controller ) {
</span><span class='line'>      $this-&gt;_c
</span><span class='line'> }
</span><span class='line'>
</span><span class='line'> if( !$this-&gt;_method ) {
</span><span class='line'>     $this-&gt;_method = 'index';
</span><span class='line'> }
</span><span class='line'>&lt;/code>&lt;/pre>
</span><span class='line'>
</span><span class='line'>&lt;p> } //&lt;&mdash; * End * &ndash;>&lt;/p>
</span><span class='line'>
</span><span class='line'>&lt;p> public function getController() {
</span><span class='line'>     return $this->_controller;
</span><span class='line'> }&lt;/p>
</span><span class='line'>
</span><span class='line'>&lt;p> public function getMethod() {
</span><span class='line'>     return $this->_method;
</span><span class='line'> }&lt;/p>&lt;/li>
</span><span class='line'>&lt;/ul>
</span><span class='line'>
</span><span class='line'>
</span><span class='line'>&lt;p>} //&lt;&ndash; * End Class * &ndash;>&lt;/p>
</span><span class='line'>
</span><span class='line'>&lt;p>/&lt;em> End of file Router.php &lt;/em>/
</span><span class='line'>/&lt;em> Location: ./base/Router.php &lt;/em>/</span></code></pre></td></tr></table></div></figure></p>

