</span><span class='line'>    <span class="nb">puts</span> <span class="n">purple</span><span class="p">(</span><span class="n">i</span><span class="o">.</span><span class="n">to_s</span><span class="p">)</span> <span class="o">+</span> <span class="o">&amp;</span><span class="n">ldquo</span><span class="p">;</span> <span class="p">:</span> <span class="o">&amp;</span><span class="n">rdquo</span><span class="p">;</span> <span class="o">+</span> <span class="n">yellow</span><span class="p">(</span><span class="n">themeCollection</span><span class="o">[</span><span class="n">i</span><span class="o">]</span><span class="p">);</span>
</span><span class='line'><span class="k">end</span>
</span><span class='line'><span class="nb">puts</span> <span class="o">&amp;</span><span class="n">ldquo</span><span class="p">;\</span><span class="n">n</span><span class="o">&amp;</span><span class="n">rdquo</span><span class="p">;;</span>
</span><span class='line'><span class="k">begin</span>
</span><span class='line'>    <span class="c1">#trying to get a valid url</span>
</span><span class='line'>    <span class="k">if</span> <span class="n">urlInvalid</span> <span class="o">==</span> <span class="kp">false</span>
</span><span class='line'>        <span class="nb">puts</span> <span class="n">green</span><span class="p">(</span><span class="o">&amp;</span><span class="n">ldquo</span><span class="p">;</span><span class="no">Tolong</span> <span class="n">masukan</span> <span class="n">url</span> <span class="n">yang</span> <span class="n">valid</span><span class="o">&amp;</span><span class="n">rdquo</span><span class="p">;);</span>
</span><span class='line'>    <span class="k">else</span>
</span><span class='line'>        <span class="nb">puts</span> <span class="n">red</span><span class="p">(</span><span class="o">&amp;</span><span class="n">ldquo</span><span class="p">;</span><span class="n">url</span> <span class="n">tidak</span> <span class="n">valid</span><span class="o">&amp;</span><span class="n">rdquo</span><span class="p">;);</span>
</span><span class='line'>    <span class="k">end</span>
</span><span class='line'>    <span class="n">urlName</span> <span class="o">=</span> <span class="nb">gets</span><span class="o">.</span><span class="n">gsub</span><span class="p">(</span><span class="o">&amp;</span><span class="n">ldquo</span><span class="p">;\</span><span class="n">n</span><span class="o">&amp;</span><span class="n">rdquo</span><span class="p">;,</span><span class="o">&amp;</span><span class="n">ldquo</span><span class="p">;</span><span class="o">&amp;</span><span class="n">rdquo</span><span class="p">;);</span>
</span><span class='line'>    <span class="k">if</span><span class="p">(</span><span class="n">valid?</span><span class="p">(</span><span class="n">urlName</span><span class="p">)</span> <span class="o">===</span> <span class="kp">true</span><span class="p">)</span>
</span><span class='line'>        <span class="n">isUrlValid</span> <span class="o">=</span> <span class="mi">0</span><span class="p">;</span>
</span><span class='line'>    <span class="k">end</span><span class="p">;</span>
</span><span class='line'>    <span class="n">urlInvalid</span><span class="o">=</span><span class="kp">true</span><span class="p">;</span>
</span><span class='line'><span class="k">end</span> <span class="k">while</span> <span class="n">isUrlValid</span> <span class="o">!=</span> <span class="mi">0</span><span class="p">;</span><span class="o">&lt;</span><span class="sr">/p&gt;</span>
</span><span class='line'>
</span><span class='line'><span class="sr">&lt;h1&gt;url now valid, tell the user&lt;/</span><span class="n">h1</span><span class="o">&gt;</span>
</span><span class='line'>
</span><span class='line'><span class="o">&lt;</span><span class="nb">p</span><span class="o">&gt;</span><span class="nb">puts</span> <span class="n">yellow</span><span class="p">(</span><span class="o">&amp;</span><span class="n">ldquo</span><span class="p">;</span><span class="no">Situs</span> <span class="n">yang</span> <span class="n">vulnerable</span> <span class="n">adalah</span> <span class="p">:</span> <span class="o">&amp;</span><span class="n">rdquo</span><span class="p">;)</span> <span class="o">+</span> <span class="n">cyan</span><span class="p">(</span><span class="n">urlName</span><span class="p">);</span>
</span><span class='line'><span class="k">begin</span>
</span><span class='line'>    <span class="c1">#trying to get theme identifier</span>
</span><span class='line'>    <span class="k">if</span> <span class="n">themeInvalid</span> <span class="o">==</span> <span class="kp">false</span>
</span><span class='line'>        <span class="nb">puts</span> <span class="n">green</span><span class="p">(</span><span class="o">&amp;</span><span class="n">ldquo</span><span class="p">;</span><span class="no">Tolong</span> <span class="n">masukan</span> <span class="n">tema</span> <span class="n">yang</span> <span class="n">digunakan</span><span class="o">&amp;</span><span class="n">rdquo</span><span class="p">;);</span><span class="o">&lt;</span><span class="sr">/p&gt;</span>
</span><span class='line'>
</span><span class='line'><span class="sr">&lt;pre&gt;&lt;code&gt;else</span>
</span><span class='line'><span class="sr">    puts red(&quot;tema tidak valid&quot;);</span>
</span><span class='line'><span class="sr">end</span>
</span><span class='line'><span class="sr">themeName = gets.gsub(&quot;\n&quot;,&quot;&quot;);</span>
</span><span class='line'><span class="sr">if themeName.scan(/</span><span class="p">(</span><span class="o">[^</span><span class="mi">0</span><span class="o">-</span><span class="mi">9</span><span class="o">]+</span><span class="p">)</span><span class="o">/</span><span class="p">)</span><span class="o">.</span><span class="n">count</span> <span class="o">==</span> <span class="mi">0</span> <span class="ow">and</span> <span class="n">themeName</span> <span class="o">!=</span> <span class="s2">&quot;&quot;</span>
</span><span class='line'>    <span class="k">if</span> <span class="n">themeName</span><span class="o">.</span><span class="n">to_i</span> <span class="o">&amp;</span><span class="n">lt</span><span class="p">;</span> <span class="mi">13</span> <span class="ow">and</span> <span class="n">themeName</span><span class="o">.</span><span class="n">to_i</span> <span class="o">&amp;</span><span class="n">gt</span><span class="p">;</span> <span class="o">-</span><span class="mi">1</span>
</span><span class='line'>        <span class="n">isThemeValid</span> <span class="o">=</span> <span class="mi">0</span><span class="p">;</span>
</span><span class='line'>    <span class="k">end</span>
</span><span class='line'><span class="k">end</span>
</span><span class='line'><span class="n">themeInvalid</span> <span class="o">=</span> <span class="kp">true</span><span class="p">;</span>
</span><span class='line'><span class="o">&lt;</span><span class="sr">/code&gt;&lt;/</span><span class="n">pre</span><span class="o">&gt;</span>
</span><span class='line'>
</span><span class='line'><span class="o">&lt;</span><span class="nb">p</span><span class="o">&gt;</span><span class="k">end</span> <span class="k">while</span> <span class="n">isThemeValid</span> <span class="o">!=</span> <span class="mi">0</span><span class="p">;</span><span class="o">&lt;</span><span class="sr">/p&gt;</span>
</span><span class='line'>
