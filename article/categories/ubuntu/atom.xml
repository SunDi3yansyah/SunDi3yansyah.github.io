</span><span class='line'>
</span><span class='line'><span class="o">&lt;</span><span class="nb">p</span><span class="o">&gt;</span><span class="nb">puts</span> <span class="n">yellow</span><span class="p">(</span><span class="o">&amp;</span><span class="n">ldquo</span><span class="p">;</span><span class="no">Tema</span> <span class="n">yang</span> <span class="n">terpilih</span> <span class="n">adalah</span> <span class="p">:</span> <span class="o">&amp;</span><span class="n">rdquo</span><span class="p">;)</span> <span class="o">+</span> <span class="n">cyan</span><span class="p">(</span><span class="n">themeCollection</span><span class="o">[</span><span class="n">themeName</span><span class="o">.</span><span class="n">to_i</span><span class="o">]</span><span class="p">);</span><span class="o">&lt;</span><span class="sr">/p&gt;</span>
</span><span class='line'>
</span><span class='line'><span class="sr">&lt;h1&gt;download the content using post method&lt;/</span><span class="n">h1</span><span class="o">&gt;</span>
</span><span class='line'>
</span><span class='line'><span class="o">&lt;</span><span class="nb">p</span><span class="o">&gt;</span><span class="n">c</span> <span class="o">=</span> <span class="no">Curl</span><span class="o">.</span><span class="n">post</span><span class="p">(</span> <span class="n">urlName</span> <span class="o">+</span> <span class="o">&amp;</span><span class="n">ldquo</span><span class="p">;</span><span class="o">/</span><span class="n">wp</span><span class="o">-</span><span class="n">content</span><span class="o">/</span><span class="n">themes</span><span class="o">/&amp;</span><span class="n">rdquo</span><span class="p">;</span><span class="o">+</span><span class="n">themeCollection</span><span class="o">[</span><span class="n">themeName</span><span class="o">.</span><span class="n">to_i</span><span class="o">]</span> <span class="o">+</span> <span class="o">&amp;</span><span class="n">ldquo</span><span class="p">;</span><span class="o">/</span><span class="n">lib</span><span class="o">/</span><span class="n">scripts</span><span class="o">/</span><span class="n">dl</span><span class="o">-</span><span class="n">skin</span><span class="o">.</span><span class="n">php</span><span class="o">&amp;</span><span class="n">rdquo</span><span class="p">;,{</span>
</span><span class='line'>                        <span class="o">&amp;</span><span class="n">lsquo</span><span class="p">;</span><span class="n">_mysite_download_skin</span><span class="o">&amp;</span><span class="n">rsquo</span><span class="p">;</span> <span class="o">=&gt;</span> <span class="o">&amp;</span><span class="n">lsquo</span><span class="p">;</span><span class="o">.</span><span class="n">.</span><span class="o">/.</span><span class="n">.</span><span class="o">/.</span><span class="n">.</span><span class="o">/.</span><span class="n">.</span><span class="o">/.</span><span class="n">.</span><span class="o">/</span><span class="n">wp</span><span class="o">-</span><span class="n">config</span><span class="o">.</span><span class="n">php</span><span class="o">&amp;</span><span class="n">rsquo</span><span class="p">;</span>
</span><span class='line'>                        <span class="p">})</span> <span class="k">do</span> <span class="o">|</span><span class="n">curl</span><span class="o">|</span>
</span><span class='line'>        <span class="n">curl</span><span class="o">.</span><span class="n">ssl_verify_peer</span> <span class="o">=</span> <span class="kp">false</span>
</span><span class='line'>        <span class="n">curl</span><span class="o">.</span><span class="n">headers</span><span class="o">[&amp;</span><span class="n">ldquo</span><span class="p">;</span><span class="no">User</span><span class="o">-</span><span class="no">Agent</span><span class="o">&amp;</span><span class="n">rdquo</span><span class="p">;</span><span class="o">]</span> <span class="o">=</span> <span class="o">&amp;</span><span class="n">ldquo</span><span class="p">;</span><span class="no">BlackBerry</span><span class="o">/</span><span class="mi">3</span><span class="o">.</span><span class="mi">5</span><span class="o">.</span><span class="mi">0</span><span class="o">&amp;</span><span class="n">rdquo</span><span class="p">;</span>
</span><span class='line'>    <span class="k">end</span><span class="o">&lt;</span><span class="sr">/p&gt;</span>
</span><span class='line'>
</span><span class='line'><span class="sr">&lt;h1&gt;assign the result&lt;/</span><span class="n">h1</span><span class="o">&gt;</span>
</span><span class='line'>
</span><span class='line'><span class="o">&lt;</span><span class="nb">p</span><span class="o">&gt;</span><span class="n">text</span> <span class="o">=</span> <span class="n">c</span><span class="o">.</span><span class="n">body_str</span><span class="p">;</span><span class="o">&lt;</span><span class="sr">/p&gt;</span>
</span><span class='line'>
</span><span class='line'><span class="sr">&lt;h1&gt;tell the user, what title we use for write to file&lt;/</span><span class="n">h1</span><span class="o">&gt;</span>
</span><span class='line'>
</span><span class='line'><span class="o">&lt;</span><span class="nb">p</span><span class="o">&gt;</span><span class="k">begin</span>
</span><span class='line'>    <span class="nb">puts</span> <span class="n">green</span><span class="p">(</span><span class="o">&amp;</span><span class="n">ldquo</span><span class="p">;</span><span class="no">Please</span> <span class="n">specify</span> <span class="n">a</span> <span class="n">file</span> <span class="nb">name</span> <span class="p">(</span><span class="n">a</span><span class="o">-</span><span class="n">zA</span><span class="o">-</span><span class="no">Z0</span><span class="o">-</span><span class="mi">9</span><span class="p">)</span><span class="o">&amp;</span><span class="n">rdquo</span><span class="p">;);</span>
</span><span class='line'>    <span class="n">fileName</span> <span class="o">=</span> <span class="nb">gets</span><span class="o">.</span><span class="n">gsub</span><span class="p">(</span><span class="o">&amp;</span><span class="n">ldquo</span><span class="p">;\</span><span class="n">n</span><span class="o">&amp;</span><span class="n">rdquo</span><span class="p">;,</span><span class="o">&amp;</span><span class="n">ldquo</span><span class="p">;</span><span class="o">&amp;</span><span class="n">rdquo</span><span class="p">;);</span>
</span><span class='line'>    <span class="n">isFileNameValid</span> <span class="o">=</span> <span class="n">fileName</span><span class="o">.</span><span class="n">scan</span><span class="p">(</span><span class="sr">/([^a-zA-Z0-9]+)/</span><span class="p">)</span><span class="o">.</span><span class="n">length</span><span class="p">;</span>
</span><span class='line'><span class="k">end</span> <span class="k">while</span> <span class="n">isFileNameValid</span> <span class="o">!=</span> <span class="mi">0</span><span class="o">&lt;</span><span class="sr">/p&gt;</span>
</span><span class='line'>
</span><span class='line'><span class="sr">&lt;h1&gt;title ok tell the user&lt;/</span><span class="n">h1</span><span class="o">&gt;</span>
</span><span class='line'>
</span><span class='line'><span class="o">&lt;</span><span class="nb">p</span><span class="o">&gt;</span><span class="nb">puts</span> <span class="n">yellow</span><span class="p">(</span><span class="o">&amp;</span><span class="n">ldquo</span><span class="p">;</span><span class="no">Judul</span> <span class="n">file</span> <span class="n">yang</span> <span class="n">terpilih</span> <span class="n">adalah</span> <span class="p">:</span> <span class="o">&amp;</span><span class="n">rdquo</span><span class="p">;)</span> <span class="o">+</span> <span class="n">cyan</span><span class="p">(</span><span class="n">fileName</span><span class="p">);</span><span class="o">&lt;</span><span class="sr">/p&gt;</span>
</span><span class='line'>
</span><span class='line'><span class="sr">&lt;pre&gt;&lt;code&gt;        f = File.open(fileName + &quot;.txt&quot;, &#39;w&#39;)</span>
</span><span class='line'><span class="sr">        f.write(text)</span>
</span><span class='line'><span class="sr">        f.close</span>
</span><span class='line'><span class="sr">        puts fileName + &quot;.txt - &quot; + green(&quot;Downloaded&quot;)</span>
</span><span class='line'><span class="sr">&lt;/</span><span class="n">code</span><span class="o">&gt;&lt;</span><span class="sr">/pre&gt;</span>
</span><span class='line'>
</span><span class='line'><span class="sr">&lt;h1&gt;end step&lt;/</span><span class="n">h1</span><span class="o">&gt;</span>
</span><span class='line'>
</span><span class='line'><span class="o">&lt;</span><span class="nb">p</span><span class="o">&gt;</span>
</span></code></pre></td></tr></table></div></figure></p>

